{
  "name": "vitepress-api-document-theme",
  "private": false,
  "description": "Theme for vitepress document",
  "version": "0.1.0",
  "license": "MIT",
  "author": "LogicSpark <info@logicspark.com>",
  "homepage": "https://logicspark.com",
  "url": "https://github.com/logicspark/vitepress-api-document-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/logicspark/vitepress-api-document-theme.git"
  },
  "bugs": {
    "url": "https://github.com/logicspark/vitepress-api-document-theme/issues"
  },
  "keywords": [
    "header",
    "footer",
    "vuejs",
    "vue",
    "vue3"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vitepress-api-document-theme.umd.js",
  "module": "./dist/vitepress-api-document-theme.es.js",
  "exports": {
    ".": {
      "import": "./dist/vitepress-api-document-theme.es.js",
      "require": "./dist/vitepress-api-document-theme.umd.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/components dist/types",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vite-plugin-css-injected-by-js": "^3.1.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/test-utils": "^2.3.2",
    "jsdom": "^21.1.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": ">=18 <19"
  }
}
